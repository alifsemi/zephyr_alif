	clock: clock-controller@4903f000 {
		compatible = "alif,clk";
		reg = <0x4903F000 0xB0
				0x4902F000 0xBC
				0x1A604000 0x44
				0x1A609000 0x14
				0x43007000 0x2C
				0x400F0000 0x14>;
		reg-names = "master_clkctrl",
				"slave_clkctrl",
				"aon_clkctrl",
				"vbat_clkctrl",
				"m55he_clkctrl",
				"m55hp_clkctrl";
		#clock-cells = <1>;
		status = "okay";
	};
	ethosu {
	#address-cells = <1>;
	#size-cells = <0>;
	interrupt-parent = <&nvic>;
		ethosu0: ethosu55@400E1000 {
			compatible = "arm,ethos-u";
			reg = <0x400E1000>;
			interrupts = <55 3>;
			secure-enable;
			privilege-enable;
			status = "okay";
		};
	};
	peripheral@40000000 {
		#address-cells = < 0x1 >;
		#size-cells = < 0x1 >;
		ranges = < 0x0 0x40000000 0x2000000 >;

		seservice0r: mhu@40040000 {
			compatible = "arm,mhuv2";
			reg = < 0x40040000 0x1000 >;
			interrupts = < 37 3 >;
			interrupt-names = "rx";
		};
		seservice0s: mhu@40050000 {
			compatible = "arm,mhuv2";
