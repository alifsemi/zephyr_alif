#include "alif/ensemble/common/e1c.dtsi"

&sram0 {
	reg = <0x02000000 DT_SIZE_K(512)>;
};

&sram1 {
	reg = <0x08000000 DT_SIZE_K(1536)>;
};

&itcm {
	reg = <0x0 DT_SIZE_K(512)>;
};

&dtcm {
	reg = <0x20000000 DT_SIZE_K(1536)>;
};

&mram_flash {
	/* Usable MRAM size for applications is 1945 KB */
	reg = <0x80000000 DT_SIZE_K(1945)>;
};

&mram_storage {
	reg = <0x80000000 DT_SIZE_K(1945)>;
};
